<config>
	<parameters>
		<mysql>
			<host>{server_host}</host>
			<username>{username}</username>
			<password>{password}</password>
			<dbname>{database}</dbname>
		</mysql>
		<kernel>
			<ip>{kernel_ip}</ip>
			<port>{kernel_port}</port>
			<pid>{kernel_pid}</pid>
			<log>{log_general}</log>
			<debug>false</debug>
			<debug_log>{log_debug}</debug_log>
			<request_timing>3.000001</request_timing>
		</kernel>
		<timezone>{timezone}</timezone>
	</parameters>
</config>